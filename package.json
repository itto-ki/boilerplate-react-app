{
  "name": "boilerplate-react-app",
  "version": "1.0.0",
  "description": "Boilerplate React Application with Webpack",
  "main": "src/index.tsx",
  "scripts": {
    "start": "webpack-cli serve",
    "build": "webpack",
    "typed": "tcm -w -p 'src/**/*.scss' .",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint ./src",
    "lint:prettier": "prettier ./src --check",
    "lint:css": "stylelint **/*.{css,scss,sass}",
    "fix": "run-s fix:*",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn lint:prettier --write",
    "fix:css": "yarn lint:css --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itto-ki/boilerplate-react-app.git"
  },
  "author": "Ittoh Kimura",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/itto-ki/boilerplate-react-app/issues"
  },
  "homepage": "https://github.com/itto-ki/boilerplate-react-app#readme",
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.12.13",
    "@types/classnames": "^2.2.11",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "autoprefixer": "^9.8.6",
    "babel": "^6.23.0",
    "babel-loader": "^8.2.2",
    "classnames": "^2.2.6",
    "core-js": "^3.8.3",
    "css-loader": "^5.0.2",
    "eslint": "^7.19.0",
    "eslint-plugin-react": "^7.22.0",
    "html-webpack-plugin": "^5.0.0",
    "mini-css-extract-plugin": "^1.3.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.5",
    "postcss-loader": "^5.0.0",
    "prettier": "^2.2.1",
    "sass": "^1.32.6",
    "sass-loader": "^11.0.1",
    "stylelint": "^13.9.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "typed-css-modules": "^0.6.5",
    "typed-css-modules-loader": "^0.0.18",
